    "HTML Email CTA": {
        "prefix":"emcta",
        "body":[
            "<table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" role=\"presentation\">",
            "\t<tr>",
            "\t\t<td align=\"center\" style=\"background-color: ${8:#000}; width: ${9:150px}\">",
            "\t\t\t<a class=\"${3:cta}\" href=\"$2\" alias=\"$4\" target=\"_blank\" style=\"line-height: 1; color: ${5:#fff}; display: block; font-size: ${6:15px}; padding: ${7:15px 10px}; text-decoration: none;\">$1</a>",
            "\t\t</td>",
            "\t<tr>",
            "</table>"
        ],
        "description": "Creates a standard CTA, feel free to edit/remove details"
    },    

    "HTML Email Rounded CTA": {
        "prefix":"emctaRnd",
        "body":[
