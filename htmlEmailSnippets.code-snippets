            "\t\t\t\t<table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" role=\"presentation\" width=\"600\" style=\"width: 600px\">",
            "\t\t\t\t\t<tr><td align=\"${9:center}\" valign=\"top\" style=\"width: ${8:300px};\">",
            "\t\t\t<![endif]-->",
            "\t\t\t<div class=\"column\">",
            "\t\t\t<!-- add style attribute if you are using it on 4 or more columns <div class=\"column\" style=\"max-width: 300px; width: 100%;\"> -->",
            "\t\t\t\t<table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" role=\"presentation\" width=\"100%\" style=\"padding:0\">",
            "\t\t\t\t\t<tr>",
            "\t\t\t\t\t\t<td align=\"${7:center}\" style=\"padding: ${5:0}; $6\">",
            "\t\t\t\t\t\t\t<!-- Left Content -->",
            "\t\t\t\t\t\t\t<span style=\"font-size: 12px; \">Left Content</span>",
            "\t\t\t\t\t\t</td>",
            "\t\t\t\t\t</tr>",
            "\t\t\t\t</table>",
            "\t\t\t</div>",
            "\t\t\t<!--[if true]>",
            "\t\t\t\t\t</td>",
            "\t\t\t\t\t<td align=\"${14:center}\" valign=\"top\" style=\"width: ${13:300px};;\">",
            "\t\t\t<![endif]-->",
            "\t\t\t<div class=\"column\">",
            "\t\t\t<!-- add style attribute if you are using it on 4 or more columns <div class=\"column\" style=\"max-width: 300px; width: 100%;\"> -->",
            "\t\t\t\t<table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" role=\"presentation\" width=\"100%\" style=\"padding:0\">",
            "\t\t\t\t\t<tr>",
            "\t\t\t\t\t\t<td align=\"${12:center}\" style=\"padding: ${10:0}; $11\">",
